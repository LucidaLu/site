<ul class="card-text font-weight-normal list-group list-group-flush">
  {% for content in data[1] %}
    <li class="list-group-item">
      <div class="row">
          <div class="col-xs-2 cl-sm-2 col-md-2">
              <h6 class="title font-weight-bold">
                {{ content.rule }}
              </h6>
            </div>
          <div class="col-xs-10 cl-sm-10 col-md-10 mt-2 mt-md-0">
          <h6 class="ml-1 ml-md-4 font-weight-normal" style="font-size: 0.95rem">
          {% for e in content.pubs %}
          <a href="{{ e.url }}">{{ e.title }}</a><span style="padding-right:0.3rem">({{ e.cnt }})</span>
          {% endfor %}
          </h6>
        </div>
          
      </div>
    </li>
  {% endfor %}
</ul>
